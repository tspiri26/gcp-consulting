<!DOCTYPE html>
<html>
  <head>
    <title>List of Clients</title>
    <style>
      body {
        background-color: #f5f5f5;
      }
      h1 {
        color: #333;
        margin-bottom: 20px;
      }
      table {
        border-collapse: collapse;
        width: 100%;
        max-width: 800px;
        margin: 0 auto;
        background-color: #fff;
        box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
      }
      th, td {
        text-align: left;
        padding: 12px;
        border-bottom: 1px solid #ddd;
      }
      th {
        background-color: #f2f2f2;
      }
      tr:hover {
        background-color: #f5f5f5;
      }
    </style>
  </head>
  <body>
    <h1>List of Clients</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach((client) => { %>
        <tr>
          <td><%= client.name %></td>
          <td><%= client.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
