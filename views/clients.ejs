<!DOCTYPE html>
<html>
<head>
	<title>List of Clients</title>
	<link rel="stylesheet" href="style.css">
</head>
<body>
	<header>
		<h1>List of Clients</h1>
		<nav>
			<ul>
				<li><a href="/">Home</a></li>
				<li><a href="/contact">Contact Us</a></li>
				<li><a href="/documentation">Documentation</a></li>
				<li><a href="/clients">Clients</a></li>
				<li><a href="/translate">Translate-API</a></li>
			</ul>
		</nav>
	</header>
	<div class="centered">
		<ul class="client-list">
			<% results.forEach((client) => { %>
			<li>
				<img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/f4fcdcf2-99e8-4d6c-b560-1b1691949a74/d5ty2d6-aa24fa34-dad1-452b-a383-5977f6bc7443.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..3oFx9Jf4G4RUkoPhWS2RiiaYjESVvRkD7xSXQRmCRX8" alt="Client Image">
				<h3><%= client.name %></h3>
				<p><%= client.email %></p>
				<p><%= client.message %></p>
      </li>
			<% }); %>
		</ul>
	</div>
</body>
</html>
