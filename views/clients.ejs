<!DOCTYPE html>
<html>
  <head>
    <title>List of Clients</title>
    <link rel="stylesheet" href="clients.css">
  </head>
  <body>
    <h1>List of Clients</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach((client) => { %>
        <tr>
          <td><%= client.name %></td>
          <td><%= client.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
