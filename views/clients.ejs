<!DOCTYPE html>
<html>
  <head>
    <title>List of Clients</title>
    <style>
      body {
        background-color: #f2f2f2;
      }
      h1 {
        text-align: center;
        margin-top: 50px;
      }
      table {
        margin: 0 auto;
        width: 80%;
        border-collapse: collapse;
        background-color: white;
      }
      th, td {
        padding: 12px;
        text-align: left;
        border-bottom: 1px solid #ddd;
      }
      th {
        background-color: #4CAF50;
        color: white;
      }
    </style>
  </head>
  <body>
    <h1>List of Clients</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach((client) => { %>
        <tr>
          <td><%= client.name %></td>
          <td><%= client.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
